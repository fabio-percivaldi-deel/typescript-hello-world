{
  "name": "swagger-service",
  "version": "1.0.0",
  "description": "Project exposing swaggers file through swagger UI",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/src/server.js",
  "scripts": {
    "prebuild-docker": "ssh-agent && ssh-add && npm run build-docker:docker-login",
    "build-docker": "./build.docker.local.sh",
    "build-docker:docker-login": "aws ecr get-login-password --region eu-west-1 | docker login --username AWS --password-stdin 974360507615.dkr.ecr.eu-west-1.amazonaws.com",
    "start": "NODE_ENV=production npm run build && node -r @letsdeel/init dist/src/server.js",
    "dev": "nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "docker:build:m1": "docker buildx build --platform linux/amd64 .",
    "test:e2e": "node --max-old-space-size=4096 ./node_modules/.bin/jest --detectOpenHandles src/tests/.*.test.ts --expand --force-exit --logHeapUsage --no-cache",
    "test:unit": "jest src/.*.spec.ts --expand --no-cache --collect-coverage --passWithNoTests",
    "test": "tsc --noEmit && npm run test:unit && npm run test:e2e",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "migrations": "npm run build && node -r dotenv-expand/config dist/src/migrate.js",
    "migrations:create": "cp src/migrations/_dummy src/migrations/`date '+%Y%m%d%H%M%S-dummy.ts'`",
    "prepare": "husky install",
    "swagger:generate": "ts-node src/scripts/SwaggerGenerator.ts",
    "script": "LOG_LEVEL=info ASYNC_CONTEXT=TRUE node -r @letsdeel/init -r reflect-metadata dist/src/scripts/ScriptRunner.js",
    "script:dev": "NODE_ENV=development LOG_LEVEL=info ASYNC_CONTEXT=TRUE nodemon --exec ts-node -r tsconfig-paths/register -r @letsdeel/init --transpile-only ./src/scripts/ScriptRunner.ts",
    "validate:swagger": "swagger-cli validate",
    "specs:generate": "node src/scripts/specs.js",
    "preinstall": "git config core.hooksPath .githooks",
    "ts:generate": "openapi-typescript 'swaggers/**/*.{json,yaml}' --output ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letsdeel/benefits.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/letsdeel/swagger-service#readme",
  "dependencies": {
    "@abitia/zod-dto": "^0.4.0",
    "@apidevtools/swagger-cli": "^4.0.4",
    "@faker-js/faker": "^8.3.1",
    "@letsdeel/employee-tribe-common": "^1.67.2",
    "@letsdeel/init": "^1.26.1",
    "@letsdeel/nats-lib": "^1.4.0",
    "@openpgp/web-stream-tools": "^0.0.14",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sentry/node": "^7.93.0",
    "@sentry/tracing": "^7.93.0",
    "aws-sdk": "^2.1534.0",
    "axios": "^1.6.5",
    "axios-retry": "^4.0.0",
    "bignumber.js": "^9.1.2",
    "bluebird": "^3.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.3.2",
    "dotenv-expand": "^11.0.6",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "fast-csv": "^4.3.6",
    "glob": "^10.3.12",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "js-convert-case": "^4.2.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "openapi-typescript": "^6.7.4",
    "openpgp": "^5.11.0",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pino": "^8.17.2",
    "reflect-metadata": "^0.1.14",
    "routing-controllers": "^0.10.4",
    "sequelize": "^6.35.2",
    "sequelize-pool": "7.1.0",
    "sequelize-typescript": "^2.1.6",
    "ssh2-sftp-client": "^9.1.0",
    "string-similarity": "^4.0.4",
    "supertest": "^7.0.0",
